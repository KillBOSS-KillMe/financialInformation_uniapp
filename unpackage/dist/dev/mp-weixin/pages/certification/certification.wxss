page {
  width: 100vw;
  height: 100vh;
}
.content {
  width: 690rpx !important;
  padding: 30rpx;
}
.content .item {
  width: 100%;
  height: auto;
  padding: 20rpx 0;
}
.content .item view {
  font-size: 29rpx;
  font-weight: 800;
  color: #343434;
}
.content .item input {
  width: 630rpx;
  height: 80rpx;
  padding: 0 30rpx;
  margin-top: 20rpx;
  border: 1rpx solid #EEEEEE;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.upIDCard {
  width: 630rpx;
  height: auto;
  padding: 0 60rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.upIDCard image {
  width: 300rpx;
  height: 200rpx;
}
.sub {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 750rpx;
  height: auto;
}
.sub view {
  width: 700rpx;
  height: 80rpx;
  margin: 30rpx 25rpx;
  font-size: 29rpx;
  font-weight: 800;
  color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  background-color: #265ED7;
}

