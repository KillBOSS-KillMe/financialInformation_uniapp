.managerDetails .info .data .item text {
  font-size: 25rpx;
  color: #898989;
}
.managerDetails .image {
  width: 140rpx;
  height: 100%;
}
.managerDetails .image image {
  width: 100%;
  height: 170rpx;
  border-radius: 5rpx;
}
.managerDetails .image view {
